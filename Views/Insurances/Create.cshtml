@model ZaverecnyProjektForman2.Models.Insurance

@{
    ViewData["Title"] = "Vyvtořit typ pojištění";
}

<h1>Vyvtořit typ pojištění</h1>

<div class="row mt-2">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Type" class="control-label">Typ pojištění</label>
                <input asp-for="Type" class="form-control" placeholder="Zadejte typ..." />
                <span asp-validation-for="Type" class="text-danger"></span>
                <small class="form-text text-muted small-text">Název nového typu pojištění.</small>
            </div>
            <div class="form-group mt-4">
                <input type="submit" value="Uložit" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div class="mt-5">
    <button class="btn btn-outline-secondary btn-zpet" onclick="history.back()">
        Zpět
    </button>
</div>

